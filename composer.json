{
    "name": "dilab/resumable.php",
    "description": "PHP package for Resumable.js",
    "authors": [
        {
            "name": "xu ding",
            "email": "thedilab@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/log": "^2.0.0",
        "psr/http-message": "^1.0",
        "knplabs/gaufrette": "^0.11.1",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "nyholm/psr7": "1.5.1",
        "phpunit/phpunit": "^9",
        "guzzlehttp/psr7": "^2.4"
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit",
        "phpcs": "./vendor/bin/phpcs",
        "phpcbf": "./vendor/bin/phpcbf"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dilab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dilab\\Test\\": "test/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
